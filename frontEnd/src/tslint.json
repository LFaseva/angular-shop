{
  "extends": "../tslint.json",
  "rules": {
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "directive-selector": [
      true,
      "attribute",
      [
        "app",
        "toggler",
        "form"
      ],
      "kebab-case"
    ],
  }
}